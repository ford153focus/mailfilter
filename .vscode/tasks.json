{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build_ExchangeFilter",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/ExchangeFilter/ExchangeFilter.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "publish_ExchangeFilter",
            "command": "dotnet",
            "type": "process",
            "args": [
                "publish",
                "${workspaceFolder}/ExchangeFilter/ExchangeFilter.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "watch_ExchangeFilter",
            "command": "dotnet",
            "type": "process",
            "args": [
